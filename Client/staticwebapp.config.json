{
  "routes": [
    {
      "route": "/api/products/*",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    },
    {
      "route": "/.auth/login/twitter",
      "statusCode": 404
    },
    {
      "route": "/.auth/login/github",
      "statusCode": 404
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "*.{css,scss,js,png,gif,ico,jpg,svg}" ]
  }
}